<?php

namespace Loogares\UsuarioBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Loogares\UsuarioBundle\Entity\TagRecomendacion
 */
class TagRecomendacion
{
    /**
     * @var integer $id
     */
    private $id;

    /**
     * @var Loogares\UsuarioBundle\Entity\Recomendacion
     */
    private $recomendacion;

    /**
     * @var Loogares\UsuarioBundle\Entity\Tag
     */
    private $tag;


    /**
     * Set id
     *
     * @param integer $id
     */
    public function setId($id)
    {
        $this->id = $id;
    }

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set recomendacion
     *
     * @param Loogares\UsuarioBundle\Entity\Recomendacion $recomendacion
     */
    public function setRecomendacion(\Loogares\UsuarioBundle\Entity\Recomendacion $recomendacion)
    {
        $this->recomendacion = $recomendacion;
    }

    /**
     * Get recomendacion
     *
     * @return Loogares\UsuarioBundle\Entity\Recomendacion 
     */
    public function getRecomendacion()
    {
        return $this->recomendacion;
    }

    /**
     * Set tag
     *
     * @param Loogares\UsuarioBundle\Entity\Tag $tag
     */
    public function setTag(\Loogares\UsuarioBundle\Entity\Tag $tag)
    {
        $this->tag = $tag;
    }

    /**
     * Get tag
     *
     * @return Loogares\UsuarioBundle\Entity\Tag 
     */
    public function getTag()
    {
        return $this->tag;
    }
}